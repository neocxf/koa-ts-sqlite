    "@types/http-status": "^0.2.30",
    "@types/ioredis": "^3.2.13",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/koa": "^2.0.46",
    "@types/koa-router": "^7.0.30",
    "@types/koa-views": "^2.0.3",
