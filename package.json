{
  "name": "@neocxf/koa-ts-sqlite",
  "version": "1.0.0",
  "description": "JWT auth implemented using koajs with typescript",
  "main": "dist/index",
  "typings": "types/index.d.ts",
  "files": [
    "dist/",
    "types/"
  ],
  "scripts": {
    "build": "gulp scripts",
    "build:watch": "gulp watch",
    "start": "npm-run-all --parallel build:watch server",
    "server": "nodemon dist",
    "prod": "npm-run-all clean prod:build prod:server",
    "prod:server": "forever start -a --uid='auth' dist",
    "prod:build": "tsc",
    "prod:stop": "forever stop auth",
    "db": "sequelize db:migrate && sequelize db:seed:all",
    "test": "tsc && mocha dist/**/*.spec.js",
    "lint": "eslint src --ext ts",
    "clean": "rimraf -rf dist",
    "copy": "copy package.json dist"
  },
  "keywords": [],
  "author": "neo",
  "license": "MIT",
  "dependencies": {
    "async": "^2.6.1",
    "async-ratelimiter": "^1.1.1",
    "bcrypt": "^2.0.1",
    "crypto": "^1.0.1",
    "eureka-js-client": "^4.4.1",
    "forever": "^0.15.3",
    "graphql": "14.0.2",
    "handlebars": "^4.0.11",
    "http-status": "^1.2.0",
    "ioredis": "^3.2.2",
    "jsonwebtoken": "^8.3.0",
    "koa": "^2.5.1",
    "koa-body": "^4.0.4",
    "koa-compose": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-views": "^6.1.4",
    "koa2-swagger-ui": "^2.11.3",
    "mz": "^2.7.0",
    "nodemailer": "^4.6.7",
    "nodemon": "1.11.0",
    "sequelize": "^4.38.0",
    "sqlite3": "^4.0.1",
    "swagger-jsdoc": "^1.10.2",
    "swagger-koa": "^2.0.0"
  },
  "devDependencies": {
    "@types/async": "^2.0.49",
